type User {
    id: ID!
    name: String!
    email: String!
    password: String
    createdAt: Time
    updatedAt: Time
    discardedAt: Time
}

input CreateUserRequest {
    name: String!
    email: String!
    password: String!
}

type CreateUserResponse {
    id : String!
}

input FindUserByEmailAndPasswordRequest {
    email: String!
    password: String!
}
